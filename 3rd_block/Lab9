#include <stdio.h>
#include <string.h>

int main() {

    char string1[128];

    int count0 = 0, count1 = 0, count2 = 0;

    scanf("%s", &string1);

    for (int i = 0; i < strlen(string1); i++) {

        if (string1[i] >= '0' && string1[i] <= '9')

            count0 += 1;

        if (string1[i] >= 'a' && string1[i] <= 'z')

            count1 += 1;

        if (string1[i] >= 'A' && string1[i] <= 'Z')

            count2 += 1;
        if ()

    }

    printf("count0 = %d\n", count0);

    printf("count1 = %d\n", count1);

    printf("count2 = %d\n", count2);
}
